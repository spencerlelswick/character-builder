<%- include('../partials/header.ejs') %>

  <p>CREATE YOUR CHARACTERS <%= title %>
  </p>

  <div id="createArea" >
    <% characters.forEach(char=> { %>
      <div class="char">
        <h2><%- char.name %></h2>
        <p><%- char.race %> <%- char.charClass %></p>
        <a href="characters/<%- char._id %>">INFO</a>
      </div>
      <% }) %>
        <a class="charBtn" href="characters/new">+</a>
  </div>



  <%- include('../partials/footer.ejs') %>